doctype html
head
  meta(charset='UTF-8')
  link(rel='stylesheet' href='https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css' integrity='sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2' crossorigin='anonymous')
  link(rel='stylesheet' src='/css/project.css')
  link(rel="stylesheet", href="/css/style.css")
  meta(name='viewport' content='width=device-width, initial-scale=1.0')
  title COVID-19 Registration from
.headerImage
  img(src='/images/form-header-image.png')
.MoH
  h6
    | Minstry of Health
.COVID
  h1 COVID-19 (CORONA VIRUS)
.Regist
  h6
    | Virus-Test Registration form
.Form
  form(name='=patForm' action='/' method='POST')
    .row
      .col
        label Surname
        input.form-control(type='text' name='surname')
      span#sError
      .col
        label Given name
        input.form-control(type='text' name='gname')
      span#GnameError
    .row
      .col
        label Date of Birth
        input.form-control(type='date' name='dob')
      span#DoBError
      .col
        label Placeo of Birth
        input.form-control(type='text' name='PoB')
    .row
      .col
        label Occupation
        input.form-control(type='text' name='occup')
      span#OccError
      .col
        label Nationality
        input.form-control(type='text' name='nation')
      span#NationError
    .row
      .col
        label Gender
        input#inlineRadio1.form-check-input(type='radio' name='inlineRadioOptions' value='Male')
        label.form-check-label(for='inlineRadio1') Male
        //
          <input class=" col form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio1" value="option1">
          <label class="form-check-label" for="inlineRadio1">1</label>
      .col
        input#inlineRadio1.form-check-input(type='radio' name='inlineRadioOptions' value='option1')
        label.form-check-label(for='inlineRadio1') Female
    .mb-3
      select.custom-select(name='Default')
        option(value='Returnee') Returnee
        option(value='Returnee') Contact
        option(value='Contact') Alert
        option(value='Volunteer') Volunteer
      span#NationError
button.btn.btn-primary.btn-lg.btn-block(type='submit') Register
script(src='https://code.jquery.com/jquery-3.5.1.slim.min.js' integrity='sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj' crossorigin='anonymous')
script(src='https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js' integrity='sha384-ho+j7jyWK8fNQe+A12Hb8AhRq26LrZ/JpcUGGOn+Y7RsweNrtN/tE3MoK7ZeZDyx' crossorigin='anonymous')
script(src='/js/custom.js')
